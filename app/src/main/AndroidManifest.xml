<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.templatepage">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/CustomActionBarTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".requestActivity"
            android:parentActivityName=".MainActivity"
            android:label="Request" />
        <activity android:name=".reportActivity"
            android:parentActivityName=".MainActivity"
            android:label="Report" />
        <activity android:name="com.example.newsedu.NewsEdu"
            android:parentActivityName=".MainActivity"
            android:label="NewsEdu"/>
        <activity android:name=".categoryActivity"
            android:parentActivityName=".MainActivity"
            android:label="Category" />
        <activity android:name=".GarbageClassificationSpec"
            android:parentActivityName=".categoryActivity"
            android:label="Category" />
        <activity android:name=".scheduleActivity"
            android:parentActivityName=".MainActivity"
            android:label="Category" />

    </application>

</manifest>